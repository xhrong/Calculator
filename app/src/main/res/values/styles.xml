<resources xmlns:android="http://schemas.android.com/apk/res/android">
    
    

    <style name="AppTheme" parent="android:Theme.Holo.Light">
        <item name="android:buttonStyle">@style/Theme.AppTheme.Widget.Button</item>
        <item name="android:windowBackground">@color/window_bg</item>
        <item name="android:background">#00F2F2EC</item>
        <item name="android:actionBarStyle">@style/ActionBar</item>
        <item name="android:windowContentOverlay">@null</item>
    </style>

    <style name="Theme.AppTheme.Widget.Button" parent="android:style/Widget.Button">
        <item name="android:background">@color/normal_button_bg</item>
        <item name="android:padding">0dp</item>
        <item name="android:textColor">@color/normal_button_text</item>
        <item name="android:textSize">@dimen/norm_button_text</item>
    </style>

    <style name="ResultTheme" parent="android:style/Widget.EditText">
        <item name="android:padding">5dp</item>
        <item name="android:textSize">40sp</item>
        <item name="android:background">@drawable/textfield_darkblue</item>
        <item name="android:textColor">#fff</item>
    </style>

    <style name="norm_buttons" parent="Theme.AppTheme.Widget.Button"></style>

    <style name="AdvBut" parent="android:style/Widget.Button">
        <item name="android:background">@color/adv_button_bg</item>
        <item name="android:textSize">@dimen/adv_button_text</item>
        <item name="android:padding">@dimen/adv_button_padding</item>
        <item name="android:textColor">@color/adv_button_text</item>
        <item name="android:minHeight">48dp</item>
    </style>

    <style name="NormButLand" parent="Theme.AppTheme.Widget.Button">
        <item name="android:padding">0dp</item>
    </style>

    <style name="ActionBar" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:background">@color/transparent</item>
        <item name="android:titleTextStyle">@style/TitleText</item>
        <item name="android:logo">@drawable/logo_default</item>
        <item name="android:textColor">#35444f</item>
    </style>

    <style name="TitleText" parent="AppTheme">
        <item name="android:textColor">#35444f</item>
        <item name="android:textSize">20sp</item>
        <item name="android:background">#00F2F2EC</item>
    </style>

